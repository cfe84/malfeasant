{
  "testEnvironment": "node",
  "testMatch": [
    "**/tests/**/*.test.js"
  ],
  "collectCoverageFrom": [
    "src/**/*.js",
    "RobotDetectorV2.js",
    "!src/**/index.js",
    "!**/node_modules/**"
  ],
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ],
  "setupFilesAfterEnv": [
    "./tests/setup.js"
  ],
  "testTimeout": 10000
}